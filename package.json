{
  "private": true,
  "name": "ionic-app-starter",
  "version": "0.1.0-dev",
  "description": "Easily start a complex Ionic 1 application",
  "engines": {
    "node": ">=4.2.0"
  },
  "devDependencies": {
    "bower": "1.7.9",
    "cordova": "6.4.0",
    "cross-env": "3.1.3",
    "del": "2.2.2",
    "@types/angular": "1.5.23",
    "@types/angular-dynamic-locale": "0.1.33",
    "@types/angular-translate": "2.15.1",
    "@types/angular-ui-router": "1.1.37",
    "@types/ionic": "0.0.34",
    "@types/lodash": "3.10.1",
    "@types/moment": "2.11.29",
    "@types/moment-timezone": "0.2.28",
    "@types/ng-cordova": "0.0.12",
"eslint": "3.1.1",
"eslint-config-fingerproof": "3.0.0",
    "gulp": "3.9.1",
    "gulp-angular-filesort": "1.1.1",
    "gulp-angular-templatecache": "2.0.0",
    "gulp-angular-translate": "0.1.4",
    "gulp-batch": "1.0.5",
    "gulp-changed": "1.3.2",
    "gulp-cheerio": "0.6.2",
    "gulp-clean-css": "2.0.13",
    "gulp-connect": "5.0.0",
    "gulp-data": "1.2.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-if": "2.0.1",
    "gulp-inject": "4.1.0",
    "gulp-pug": "3.1.0",
    "gulp-load-plugins": "1.3.0",
    "gulp-load-tasks": "rummik/gulp-load-tasks#cedeb08",
    "gulp-ng-annotate": "2.0.0",
    "gulp-ng-constant": "2.0.0-2",
    "gulp-protractor": "3.0.0",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-rev": "7.1.2",
    "gulp-rev-delete-original": "0.2.2",
    "gulp-rev-format": "1.0.4",
    "gulp-rev-replace": "0.4.3",
    "gulp-sass": "2.3.2",
    "gulp-uglify": "2.0.0",
    "gulp-sourcemaps": "2.6.0",
    "gulp-typescript": "3.2.1",
    "gulp-useref": "3.1.2",
    "gulp-util": "3.0.7",
    "gulp-watch": "4.3.10",
"ionic": "1.7.15",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-spec-reporter": "0.0.26",
"lodash": "3.10.1",
    "main-bower-files": "2.13.1",
    "merge-stream": "1.0.0",
    "phantomjs-prebuilt": "2.1.13",
    "protractor": "4.0.10",
    "shelljs": "0.7.5"
    "tslint": "5.5.0",
    "typescript": "2.4.1"
  },
  "optionalDependencies": {
    "ios-deploy": "1.9.0",
    "ios-sim": "5.0.10"
  },
  "scripts": {
    "restore:web": "gulp install && gulp",
    "restore": "npm run restore:web && cordova prepare",

    "test": "gulp test",
    "test:e2e": "gulp test.e2e",
    "test:unit": "gulp test.unit",
    "test:once": "cross-env build=test npm run test",

    "serve:smartphone": "ionic serve -o /smartphone.html",
    "serve:tablet": "ionic serve -o /tablet.html",
    "serve:web": "ionic serve -o /index.html",
    "start": "npm run serve:smartphone",

    "android": "gulp && ionic run android --device",
    "android:emulator": "gulp && ionic run android --emulator",
    "ios": "gulp && ionic run ios --device",
    "ios:emulator": "gulp && ionic run ios --emulator",

    "prepare:build:web": "npm run test:once && gulp build",
    "prepare:build": "npm run restore && npm run prepare:build:web",

    "build:web": "npm run restore:web && npm run prepare:build:web",
    "build:web:preprod": "cross-env build=preprod npm run build:web",
    "build:web:prod": "cross-env build=prod npm run build:web",
    "build:web:test": "cross-env build=test npm run build:web",
    "build:web:qa": "cross-env build=qa npm run build:web",

    "build:android": "npm run prepare:build && cordova build android",
    "build:android:preprod": "cross-env build=preprod npm run build:android",
    "build:android:prod": "cross-env build=prod npm run prepare:build && cordova build android --release --buildConfig=build.json --prod",
    "build:android:test": "cross-env build=test npm run build:android",
    "build:android:qa": "cross-env build=qa npm run build:android",

    "build:ios": "npm run prepare:build && cordova build ios",
    "build:ios:preprod": "cross-env build=preprod npm run build:ios",
    "build:ios:prod": "cross-env build=prod npm run build:ios",
    "build:ios:test": "cross-env build=test npm run build:ios",
    "build:ios:qa": "cross-env build=qa npm run build:ios"
  },
  "cordovaPlatforms": [
    "ios@4.3.0",
    "android@6.1.0"
  ],
  "cordovaPlugins": [
    "cordova-plugin-console@1.0.4",
    "cordova-plugin-device@1.1.3",
    "cordova-plugin-inappbrowser@1.5.0",
    "cordova-plugin-splashscreen@4.0.0",
    "cordova-plugin-statusbar@2.2.0",
    "cordova-plugin-whitelist@1.3.0",
    "ionic-plugin-keyboard@2.2.1",
    { "locator": "./assets/plugins/cordova-plugin-native-overrides" }
  ]
}
